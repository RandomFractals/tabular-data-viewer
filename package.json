{
	"name": "tabular-data-viewer",
	"displayName": "Tabular Data Viewer ä¸­",
	"description": "Tabular Data Viewer ðŸ€„ provides custom Table View for .csv, .tsv and .tab data files with Tabulator.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.63.0"
	},
	"icon": "resources/icons/tabular-data-viewer.png",
	"publisher": "RandomFractalsInc",
	"author": "Taras Novak",
	"contributors": [
		"Taras Novak"
	],
	"license": "Apache-2.0",
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/RandomFractals/tabular-data-viewer"
	},
	"bugs": "https://github.com/RandomFractals/tabular-data-viewer/issues",
	"homepage": "https://github.com/RandomFractals/tabular-data-viewer/README.md",
	"galleryBanner": {
		"color": "#333",
		"theme": "dark"
	},
	"categories": [
		"Data Science",
		"Visualization"
	],
	"keywords": [
		"tabular data",
		"data grid ä¸­",
		"data viewer",
		"CSV",
		"TSV",
		"flat data",
		"text data",
		"table view"
	],
	"activationEvents": [
		"onLanguage:csv",
		"onLanguage:tsv",
		"onCommand:tabular.data.viewTable",
		"onCustomEditor:tabular.data.tableEditor",
		"onWebviewPanel:tabular.data.tableView"
	],
	"main": "./dist/extension.js",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "tabular.data.viewTable",
				"title": "View Table",
				"category": "Tabular Data"
			}
		],
		"languages": [
			{
				"id": "csv",
				"extensions": [
					".csv"
				],
				"aliases": [
					"CSV"
				]
			},
			{
				"id": "tsv",
				"extensions": [
					".tsv",
					".tab"
				],
				"aliases": [
					"TSV"
				]
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == csv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == tsv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"when": "resourceLangId == csv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == tsv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"when": "editorLangId == csv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				},
				{
					"when": "editorLangId == tsv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"when": "editorLangId == csv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				},
				{
					"when": "editorLangId == tsv",
					"command": "tabular.data.viewTable",
					"group": "navigation"
				}
			]
		},
		"customEditors": [
			{
				"viewType": "tabular.data.tableEditor",
				"displayName": "Table View",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.csv"
					},
					{
						"filenamePattern": "*.tsv"
					},
					{
						"filenamePattern": "*.tab"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/d3-dsv": "^3.0.0",
		"@types/vscode": "^1.63.1",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.18.0",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"eslint": "^8.5.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"ts-loader": "^9.2.6",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"@vscode/test-electron": "^2.0.0"
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "^0.8.4",
		"d3-dsv": "^3.0.1",
		"tabulator-tables": "^5.0.8"
	}
}
